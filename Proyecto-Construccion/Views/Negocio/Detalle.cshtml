@model Proyecto_Construccion.Models.Negocio

@{
    ViewData["Title"] = "Detalles del Negocio";
}

<div class="container">
    <h2>Detalles del Negocio: @Model.Nombre</h2>

    <div class="row">
        <div class="col-md-6">
            <h4>Información General</h4>
            <ul class="list-group">
                <li class="list-group-item"><strong>Nombre:</strong> @Model.Nombre</li>
                <li class="list-group-item"><strong>Tipo de Negocio:</strong> @Model.TipoNegocioId</li>
                <li class="list-group-item"><strong>Dirección:</strong> @Model.Direccion</li>
                <li class="list-group-item"><strong>Teléfono:</strong> @Model.Telefono</li>
                <li class="list-group-item"><strong>Descripción:</strong> @Model.Descripcion</li>
            </ul>
        </div>
    </div>

    
</div>
